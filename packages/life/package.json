{
  "name": "life",
  "version": "0.40.0",
  "description": "The framework to build agents that speak, write, and touch. Minimal, extensible, and typesafe.",
  "author": "Lila Rest",
  "license": "MIT+Services",
  "homepage": "https://lifejs.org",
  "bugs": {
    "url": "https://github.com/lifejs/lifejs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lifejs/lifejs"
  },
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE",
    "LICENSE.SERVICES"
  ],
  "keywords": [
    "agents",
    "framework",
    "typescript",
    "react",
    "fullstack"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "types": "tsc --noEmit --emitDeclarationOnly false",
    "test": "dotenv -e ../../.env -- vitest run",
    "prepack": "cp ../../README.md README.md && cp ../../LICENSE LICENSE && cp ../../LICENSE.SERVICES LICENSE.SERVICES"
  },
  "bin": {
    "life": "./dist/cli/index.mjs"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {},
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "@livekit/rtc-node": "^0.13.21",
    "@paralleldrive/cuid2": "^3.0.4",
    "@zenobius/ink-mouse": "^1.0.3",
    "chalk": "^5.6.2",
    "error-stack-parser": "^2.1.4",
    "fast-npm-meta": "^0.4.7",
    "imurmurhash": "^0.1.4",
    "ink": "^6.5.1",
    "ink-link": "^5.0.0",
    "livekit-client": "^2.16.0",
    "livekit-server-sdk": "^2.14.2",
    "serialize-error": "^12.0.0",
    "superjson": "^2.2.6",
    "ua-parser-js": "^2.0.6",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/imurmurhash": "^0.1.4",
    "vitest": "^4.0.15"
  }
}
